                                $("html").toggleClass('menu-ativo');

                            });


                            document.documentElement.onclick = function (event) {
                                if (event.target === document.documentElement) {
                                    fechaCarrinho()
                                    document.documentElement.classList.remove('menu-ativo');
                                }

                            };

                        } else {

