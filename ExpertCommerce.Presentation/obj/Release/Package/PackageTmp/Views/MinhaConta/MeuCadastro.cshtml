<div class="row meu-cadastro ">
    <div class="small-12 medium-10 columns medium-centered container" style="width:80%; text-align:left;">
        <br /><br />
        <form novalidate name="cadastroform" ng-submit="submitCadastro(cadastroform.$valid)" style="font-weight:bold;">
            <div class="">
                <label class="fd-label" style="width:100%">
                    <span ng-hide="!credentials.userName" style="">Nome completo</span>
                    <span ng-hide="credentials.userName" class="CorInvalid">° Nome não preenchido</span>
                    <input style="width:100%" ng-class="{ 'erroDeCadastro' : !credentials.userName}" type="text" class="fd-input fd-medium fd-lt-spacing" placeholder="Nome completo" ng-model="credentials.userName" maxlength="150" required ng-required="true" />
                    

                </label>

            </div>

            <div class="">
                <label class="fd-label MCEmail" style="">
                    <span ng-hide="!usuario.Email" >Email </span>
                    <span ng-hide="usuario.Email" class="CorInvalid">° Email não preenchido</span>
                    <input style="width:100%;" ng-class="{ 'erroDeCadastro' : !usuario.Email}" type="text" class="fd-input fd-medium fd-lt-spacing" placeholder="Email" ng-model="usuario.Email" required ng-required="true" />
                   
                </label>
                <label class="fd-label MCECPF" style="">
                    <span>CPF</span>
                    <input style="width:100%;" ng-class="{ 'erroDeCadastro' : !usuario.CNPJ}" id="cpf" ng-disabled="true" type="text" class="fd-input fd-medium fd-lt-spacing" placeholder="CPF" ng-model="usuario.CNPJ" />
                    <span ng-hide="usuario.CNPJ" class="CorInvalid">° CPF não preenchido</span>
                </label>
            </div>

            <div class="">
                <div class="">
                   
                        <label class="fd-label MCEDD" style="">
                            <span ng-hide="!usuario.DDDTelefone">DDD</span>
                            <span ng-hide="usuario.DDDTelefone" class="CorInvalid">° DDD não preenchido</span>
                            <input style="width:100%;" ng-class="{ 'erroDeCadastro' : !usuario.DDDTelefone}" id="dddtelefone" type="number" class="fd-input fd-medium fd-lt-spacing" placeholder="DDD" ng-model="usuario.DDDTelefone" required ng-required="true" />
                           
                        </label>
                  


                    
                        <label class="fd-label MCTelefone" style="">
                            <span ng-hide="!usuario.Telefone">Telefone</span>
                            <span ng-hide="usuario.Telefone" class="CorInvalid">° Telefone não preenchido</span>
                            <input style="width:100%;" ng-class="{ 'erroDeCadastro' : !usuario.Telefone}" id="telefone" type="text" class="fd-input fd-medium fd-lt-spacing" placeholder="Telefone" ng-model="usuario.Telefone" required ng-required="true" />
                           
                        </label>
                  

                  
                        <label class="fd-label MCDtNasc" style="">
                            <span ng-hide="!usuario.DataNascimento">Data de nascimento</span>
                            <span ng-hide="usuario.DataNascimento" style="color:red">° Data de nascimento não preenchida</span>
                            <input style="width:100%;" ng-class="{ 'erroDeCadastro' : !usuario.DataNascimento}" id="nascimento" type="text" class="fd-input fd-medium fd-lt-spacing" placeholder="Data de nascimento" ng-model="usuario.DataNascimento" required ng-required="true" />
                           
                        </label>
                   

                    @*<div class="small-4 columns sexo">
                            <label class="fd-label">
                                <span>Sexo</span>
                                <select ng-options="sex.tipo for sex in Sexo" class="fd-input fd-lt-spacing" placeholder="Sexo" ng-model="usuario.Sexo" required ng-required="true" tabindex="6">
                            </select>
                            </label>
                        </div>*@
                </div>


               
            </div>

            <div class="">
                <p>Todos os campos acima são obrigatórios</p>
            </div>

            <div class="grey" style="padding:3%;">
                <h1 style="text-align: center;font-weight:bold;">Altere sua senha</h1>
                <div class="" style="text-align:center;">
                    <input style="width: 60%;text-transform: inherit !important;" minlength="6" maxlength="16" type="password" class="fd-input fd-medium fd-lt-spacing" placeholder="Nova senha" ng-model="usuario.Senha" />
                </div>
                <div class="" style="text-align:center;margin-top:20px;">
                    <input style="width: 60%;text-transform: inherit !important;"  minlength="6" maxlength="16" ng-class="{ 'erroDeCadastro' : usuario.ConfirmarSenha != usuario.Senha}" type="password" class="fd-input fd-medium fd-lt-spacing" placeholder="Confirmar senha" ng-model="usuario.ConfirmarSenha" compare-to="usuario.Senha" />
                </div>


                <p ng-hide="usuario.Senha == usuario.ConfirmarSenha" class="CorInvalid" style="text-align:center;margin-top:20px;">° Senhas não conferem.</p>
                <p style="text-align:center;margin-top:20px;">°A Senha tem que ter mais de 6 digítos e menor que 16</p>



            </div>

            <div class="small-12 medium-3 columns position-right  paddingless MCBtnSalvar" style="margin-top:20px;">
                <button type="submit" class="small-12 medium-4 columns fd-button fd-primary fd-full-width " ng-disabled="cadastroform.$invalid">Salvar</button>
            </div>
        </form>
    </div>
</div>